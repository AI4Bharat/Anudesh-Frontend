{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off",
    "react/display-name": "off" ,
    "react/no-unknown-property": ["error", { "ignore": ["suppressHydrationWarning"] }],
    "no-unused-vars": ["warn", { "suppressWarnings": ["unused-imports"] }]

  }
}
